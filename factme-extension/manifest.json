{
  "manifest_version": 2,

  "name": "factme",
  "description": "This extension allows people to fact check the internet",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus"
  ]
}
